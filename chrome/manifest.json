{
  "name": "WebSocket Tester",
  "description": "Test WebSocket created by the WebSocket API and the Socket.IO API.",
  "version": "0.0.1",
  "manifest_version": 2,
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}